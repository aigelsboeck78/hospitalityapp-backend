{
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/test",
      "destination": "/api/test"
    },
    {
      "source": "/(.*)",
      "destination": "/api/serverless"
    }
  ],
  "functions": {
    "api/serverless.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/test.js": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}