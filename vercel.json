{
  "buildCommand": "npm install && npm run vercel:build",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main"
    }
  ],
  "functions": {
    "api/main.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "env": {
    "NODE_ENV": "production",
    "NODE_TLS_REJECT_UNAUTHORIZED": "0"
  }
}