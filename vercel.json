{
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main"
    }
  ],
  "functions": {
    "api/main.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}