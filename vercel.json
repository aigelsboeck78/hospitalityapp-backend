{
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/login"
    },
    {
      "source": "/api/properties",
      "destination": "/api/properties"
    },
    {
      "source": "/api/guests",
      "destination": "/api/guests"
    },
    {
      "source": "/test",
      "destination": "/api/test"
    }
  ],
  "functions": {
    "api/health.js": {
      "maxDuration": 10,
      "memory": 128
    },
    "api/auth/login.js": {
      "maxDuration": 10,
      "memory": 128
    },
    "api/properties.js": {
      "maxDuration": 10,
      "memory": 256
    },
    "api/guests.js": {
      "maxDuration": 10,
      "memory": 256
    },
    "api/test.js": {
      "maxDuration": 10,
      "memory": 128
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}