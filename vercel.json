{
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/test-events",
      "destination": "/api/test-events"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/main"
    }
  ],
  "functions": {
    "api/main.js": {
      "maxDuration": 10,
      "memory": 512
    },
    "api/test-events.js": {
      "maxDuration": 10,
      "memory": 256
    }
  },
  "env": {
    "NODE_ENV": "production",
    "NODE_TLS_REJECT_UNAUTHORIZED": "0"
  }
}